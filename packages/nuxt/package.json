  "version": "4.0.0-beta.2",
  "author": "Css Ninja <hello@cssninja.io> (https://cssninja.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuriken-ui/shuriken-ui.git",
    "directory": "packages/nuxt"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
